{
  "name": "tmi/web_cms",
  "description": "Drupal dependencies",

  "repositories": [
    {
        "type": "composer",
        "url": "https://packages.drupal.org/8"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,

  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.4.8",
      "ext-mbstring": "7.4.8",
      "ext-xml": "7.4.8",
      "ext-date": "7.4.8",
      "ext-dom": "7.4.8",
      "ext-filter": "7.4.8",
      "ext-gd": "7.4.8",
      "ext-hash": "7.4.8",
      "ext-json": "7.4.8",
      "ext-pcre": "7.4.8",
      "ext-PDO": "7.4.8",
      "ext-session": "7.4.8",
      "ext-SimpleXML": "7.4.8",
      "ext-SPL": "7.4.8",
      "ext-tokenizer": "7.4.8"
    }
  },

  "require": {
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6.5",
    "drupal-composer/drupal-scaffold": "^2.5",
    "drupal/admin_toolbar": "^2.0",
    "drupal/adminimal_admin_toolbar": "^1.7",
    "drupal/adminimal_theme": "^1.3",
    "drupal/auto_entitylabel": "^3.0@beta",
    "drupal/block_class": "^1.0",
    "drupal/boxes": "1.x-dev",
    "drupal/computed_field": "^2.0@alpha",
    "drupal/conditional_fields": "^1.0@alpha",
    "drupal/console": "^1.0.2",
    "drupal/content_access": "^1.0",
    "drupal/core": "8.8.4",
    "drupal/devel": "^2.1",
    "drupal/draggableviews": "^1.2",
    "drupal/ds": "^3.1",
    "drupal/field_group": "^3.0",
    "drupal/field_permissions": "^1.0@RC",
    "drupal/markup": "^1.0@beta",
    "drupal/menu_export": "^1.2",
    "drupal/page_manager": "^4.0@beta",
    "drupal/panels": "^4.3",
    "drupal/pathauto": "^1.2",
    "drupal/rac": "^2.0@RC",
    "drupal/search_api": "^1.15",
    "drupal/search_api_autocomplete": "^1.2",
    "drupal/slick": "^1.0",
    "drupal/slick_views": "^1.0@RC",
    "drupal/token": "^1.3",
    "drupal/viewfield": "^3.0@alpha",
    "drupal/views_accordion": "^1.2",
    "drupal/views_field_view": "^1.0@beta",
    "drupal/views_infinite_scroll": "^1.6",
    "drupal/views_tree": "2.x-dev",
    "drush/drush": "^9.0.0",
    "vlucas/phpdotenv": "^2.4",
    "webflo/drupal-finder": "^1.0.0",
    "webmozart/path-util": "^2.3"
  },

  "replace":{
    "php/ext-dom": "*"
  },

  "extra": {
    "installer-paths": {
      "libraries/{$name}": ["type:drupal-library"],
      "modules/contrib/{$name}": ["type:drupal-module"],
      "themes/contrib/{$name}": ["type:drupal-theme"]
    }
  }
}
